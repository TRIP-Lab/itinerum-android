        <selector xmlns:android="http://schemas.android.com/apk/res/android">
            <!-- Checked -->
            <item android:state_checked="true">
                <shape>
                    <stroke android:width="1dp" android:color="@color/list_item_indicator_enabled_dark"/>
                    <solid android:color="@color/base"/>
                    <padding android:left="4dp" android:top="4dp" android:right="4dp" android:bottom="4dp"/>
                    <corners android:topLeftRadius="43.5dp" android:topRightRadius="43.5dp" android:bottomLeftRadius="43.5dp" android:bottomRightRadius="43.5dp"/>
                </shape>
            </item>

            <item>
                <shape>
                    <stroke android:width="1dp" android:color="@color/background_survey"/>
                    <solid android:color="@color/background_survey"/>
                    <padding android:left="4dp" android:top="4dp" android:right="4dp" android:bottom="4dp"/>
                    <corners android:topLeftRadius="43.5dp" android:topRightRadius="43.5dp" android:bottomLeftRadius="43.5dp" android:bottomRightRadius="43.5dp"/>
                </shape>
            </item>
        </selector>
